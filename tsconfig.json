{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "/",
    "paths": {
      "~/interface/*": [
        "./src/interface/*"
      ],
      "~/constants/*": [
        "./src/constants/*"
      ],
      "~/components/*": [
        "./src/components/*"
      ],
    },
  }
}
